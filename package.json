{
  "name": "strickland",
  "version": "0.0.4",
  "description": "JavaScript data validation. Ideal for universal React applications that use Flux, Redux, or similar patterns.",
  "main": "./lib/index.js",
  "files": [ "lib/" ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run lint && npm test && babel src -d lib",
    "lint": "eslint src/** test/**",
    "test": "mocha --compilers js:babel-register --recursive",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffhandley/strickland.git"
  },
  "keywords": [
    "validation",
    "react",
    "universal",
    "flux"
  ],
  "author": "Jeff Handley <jeff@jeffhandley.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jeffhandley/strickland/issues"
  },
  "homepage": "https://github.com/jeffhandley/strickland",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "lodash": "^4.12.0"
  }
